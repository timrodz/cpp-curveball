float4 main(
	float2 vTexCoord	: SPRITECOORD,
	uniform sampler2D tex,
	uniform float4 colorCoeff
	) : COLOR
{
	float4	color = tex2D(tex, vTexCoord);
	float4	ret = color * colorCoeff;
	return	ret;
}
